{
  "name": "@usesvelte/inline-svg",
  "description": "Inline SVGs in Svelte",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/types.ts",
  "module": "./src/index.js",
  "svelte": "./src/index.js",
  "homepage": "https://github.com/usesvelte/inline-svg",
  "bugs": "https://github.com/usesvelte/inline-svg/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usesvelte/inline-svg.git"
  },
  "author": {
    "name": "Erick Vargas",
    "url": "https://github.com/eriandev"
  },
  "scripts": {
    "check:es": "eslint src/**/*.{svelte,js,ts}",
    "format": "prettier --write src/**/*.{svelte,js,ts}",
    "check:sv": "svelte-check --tsconfig ./tsconfig.json",
    "check": "pnpm format && pnpm check:sv && pnpm check:es && publint"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "svelte": "./src/index.js",
      "import": "./src/index.js"
    },
    "./types.ts": {
      "types": "./src/types.ts",
      "import": "./src/types.ts"
    }
  },
  "typesVersions": {
    "*": {
      "InlineSvg.svelte": [
        "./src/InlineSvg.svelte.d.ts"
      ]
    }
  },
  "files": [
    "src"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "keywords": [
    "svelte",
    "sveltekit",
    "inline",
    "svg"
  ],
  "engines": {
    "node": ">=22",
    "pnpm": ">=8"
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "vite": ">=5.0.0 || >=6.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.8",
    "@types/node": "22.14.1",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-svelte": "3.5.1",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "publint": "0.3.12",
    "svelte-check": "4.1.6",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}